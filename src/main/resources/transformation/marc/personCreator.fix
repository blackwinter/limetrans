# MARC/700
add_array("@person")
do list(path: "700??", "var": "$i")
  if exists("$i.[4e]")
    if any_match("$i.[4e]", "(?:[Aa]ut|[Cc]re).*")
      include("./marc/_person.fix")
    end
  else
    include("./marc/_person.fix")
  end
end
uniq("@person")
uniq("introx.person[]")
move_field("@person", "PersonCreator[]")
