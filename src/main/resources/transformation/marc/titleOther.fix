# MARC/246
set_array("TitleOther[]")
do list(path: "246??", "var": "$i")
  add_field("TitleOther[].$append.__dummy__", "")
  call_macro("remove-nonsort", source: "$i.a")
  copy_field("$i.a", "TitleOther[].$last.title")
  replace_all("$i.i", ":$", "")
  copy_field("$i.i", "TitleOther[].$last.prefix")
end
uniq("TitleOther[]")
