# MARC/100
add_array("@person")
add_array("introx.person[]")
do list(path: "100??", "var": "$i")
  if exists("$i.[4e]")
    if any_match("$i.[4e]", "(?:[Aa]ut|[Cc]re).*")
      include("./marc/_person.fix")
    end
  else
    include("./marc/_person.fix")
  end
end
uniq("@person")
move_field("@person", "Person[]")
