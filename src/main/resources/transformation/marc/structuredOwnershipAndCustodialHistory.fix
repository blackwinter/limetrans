# MARC/361
add_array("StructuredOwnershipAndCustodialHistory[]")
do list(path: "361[ 1]?", "var": "$i")
  if any_match("$i.5", "$[regexp.isil]")
    add_hash("StructuredOwnershipAndCustodialHistory[].$append")
    copy_field("$i.a", "StructuredOwnershipAndCustodialHistory[].$last.name")
    call_macro("copy-all-items", source: "$i.f", target: "StructuredOwnershipAndCustodialHistory[].$last.ownershipAndCustodialHistoryEvidenceTerm")
    copy_field("$i.k", "StructuredOwnershipAndCustodialHistory[].$last.formattedDate")
    copy_field("$i.l", "StructuredOwnershipAndCustodialHistory[].$last.date")
    call_macro("copy-all-items", source: "$i.o", target: "StructuredOwnershipAndCustodialHistory[].$last.typeOfOwnershipAndCustodialHistoryInformation")
    copy_field("$i.s", "StructuredOwnershipAndCustodialHistory[].$last.shelfMarkOfCopyDescribed")
    call_macro("copy-all-items", source: "$i.u", target: "StructuredOwnershipAndCustodialHistory[].$last.uniformResourceIdentifier")
    call_macro("copy-all-items", source: "$i.x", target: "StructuredOwnershipAndCustodialHistory[].$last.nonpublicNote")
    copy_field("$i.y", "StructuredOwnershipAndCustodialHistory[].$last.identifierOfTheCopyDescribed")
    call_macro("copy-all-items", source: "$i.z", target: "StructuredOwnershipAndCustodialHistory[].$last.publicNote")
    call_macro("copy-all-items", source: "$i.0", target: "StructuredOwnershipAndCustodialHistory[].$last.authorityRecordControlNumberOrStandardNumber")
    call_macro("copy-all-items", source: "$i.1", target: "StructuredOwnershipAndCustodialHistory[].$last.realWorldObjectURI")
    copy_field("$i.3", "StructuredOwnershipAndCustodialHistory[].$last.materialsSpecified")
    copy_field("$i.5", "StructuredOwnershipAndCustodialHistory[].$last.institutionToWhichFieldApplies")
    copy_field("$i.5", "StructuredOwnershipAndCustodialHistory[].$last.label")
    lookup("StructuredOwnershipAndCustodialHistory[].$last.label", "lobid-organisations", delete: "true")
    copy_field("$i.6", "StructuredOwnershipAndCustodialHistory[].$last.linkage")
    call_macro("copy-all-items", source: "$i.7", target: "StructuredOwnershipAndCustodialHistory[].$last.dataProvenance")
    call_macro("copy-all-items", source: "$i.8", target: "StructuredOwnershipAndCustodialHistory[].$last.fieldLinkAndSequenceNumber")
  end
end
