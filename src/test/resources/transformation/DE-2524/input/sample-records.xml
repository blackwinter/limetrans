    <datafield tag="245" ind1="1" ind2="0">
      <subfield code="a">Universitäre DaF-Lehrerausbildung in Ungarn im Spannungsfeld von Traditionen und neuen Herausforderungen</subfield>
      <subfield code="c">Ilona Feld-Knapp</subfield>
    </datafield>
    <datafield tag="250" ind1=" " ind2=" ">
