{
  "isil": "DE-A96",
  "external-maps": "/data/jenkins/limetrans/maps",
  "alma": {
    "regexp": {
      "description": ".*vorhanden.*",
      "suppressedLocation": "SUPPRESSED",
      "temporaryCallnumberLocation": ".*_(?:Lab|Sem)App"
    }
  },
  "input": {
    "queue": {
      "path": "src/test/resources/limetrans/input/",
      "pattern": "alma.xml.gz"
    }
